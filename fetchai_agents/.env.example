# ABOUTME: Environment configuration template for Fetch.ai agents.
# ABOUTME: Copy to .env and fill in your actual credentials.

# Fetch.ai Configuration
FETCHAI_API_KEY=your_fetchai_api_key_here
AGENT_SEED=your_random_seed_here_min_32_chars

# Backend API Configuration
BACKEND_API_URL=http://localhost:8000

# Pricing (in FET tokens per request)
PRICE_CONTEXT_UNDERSTANDING=0.10
PRICE_PRIVACY_REDACTION=0.05
PRICE_FOLLOWUP_GENERATION=0.08
PRICE_CROSSPOLLINATION=0.15
PRICE_QA_ROUTING=0.03
PRICE_RETRIEVAL=0.05
PRICE_INSIGHT=0.12
PRICE_RESPONSE_COMPOSE=0.05

# Agent Network Configuration
ENABLE_PAYMENT_VERIFICATION=false  # Set to true when deploying to production
AGENT_PORT_START=8001  # Starting port for agent endpoints
